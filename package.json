{
  "name": "frontend",
  "type": "module",
  "files": [
    "mass_frontend"
  ],
  "description": "The Music Assistant frontend/panel developed in Vue.",
  "main": "./mass_frontend/mass.js",
  "module": "./mass_frontend/mass.umd.cjs",
  "exports": {
    ".": {
      "import": "./mass_frontend/mass.js",
      "require": "./mass_frontend/mass.umd.js"
    }
  },
  "version": "0.0.1",
  "scripts": {
    "serve": "vite preview",
    "build": "vite build && yarn run build:finish",
    "build:finish": "replace-in-file '/src/main.ts' 'mass.umd.js' 'mass_frontend/index.html'",
    "https-preview": "serve dist",
    "test:unit": "vitest --environment jsdom",
    "lint:check": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --ignore-path .gitignore",
    "lint:fix": "eslint --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --ignore-path .gitignore --fix .",
    "dev": "vite",
    "preview": "vite build && vite preview --port 5050",
    "typecheck": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false"
  },
  "dependencies": {
    "hammerjs": "^2.0.8",
    "home-assistant-js-websocket": "^8.0.0",
    "v3-infinite-loading": "^1.2.2",
    "vue": "^3.2.45",
    "vue-horizontal": "^1.0.2",
    "vue-i18n": "^9.2.2",
    "vue-router": "^4.1.6",
    "vue-virtual-scroller": "^2.0.0-beta.3",
    "vuetify": "^3.0.1",
    "webfontloader": "^1.6.28"
  },
  "devDependencies": {
    "@intlify/vite-plugin-vue-i18n": "^6.0.3",
    "@mdi/font": "7.0.96",
    "@mdi/js": "^7.0.96",
    "@rushstack/eslint-patch": "^1.2.0",
    "@types/jsdom": "^20.0.1",
    "@types/node": "18.11.9",
    "@types/webfontloader": "^1.6.35",
    "@vitejs/plugin-vue": "^3.2.0",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^11.0.2",
    "@vue/test-utils": "^2.2.3",
    "@vue/tsconfig": "^0.1.3",
    "eslint": "^8.28.0",
    "eslint-plugin-vue": "^9.7.0",
    "https-localhost": "^4.7.1",
    "jsdom": "^20.0.2",
    "prettier": "^2.7.1",
    "replace-in-file": "^6.3.5",
    "sass": "^1.56.1",
    "sass-loader": "^13.2.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.3",
    "vite": "^3.2.4",
    "vite-plugin-pwa": "^0.13.3",
    "vite-plugin-vuetify": "^1.0.0",
    "vitest": "^0.25.2",
    "vue-cli-plugin-vuetify": "~2.5.8",
    "vue-tsc": "^1.0.9",
    "webpack-plugin-vuetify": "^2.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/music-assistant/frontend.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/music-assistant/frontend/issues"
  },
  "homepage": "https://github.com/music-assistant/frontend#readme"
}
